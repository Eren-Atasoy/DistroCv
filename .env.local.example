# =============================================================================
# LOCAL DEVELOPMENT ENVIRONMENT
# =============================================================================
# Bu dosyayÄ± .env.local.example'den kopyala:
#   cp .env.local.example .env.local
# =============================================================================

# --- Database ---
POSTGRES_DB=distrocv_dev
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# --- Backend ---
ASPNETCORE_ENVIRONMENT=Development
CONNECTION_STRING=Host=postgres;Port=5432;Database=distrocv_dev;Username=postgres;Password=postgres

# --- AWS (S3) ---
AWS_REGION=eu-west-1
AWS_S3_BUCKET=distrocv-dev-files

# --- JWT Auth ---
JWT_SECRET=distrocv-local-dev-secret-key-minimum-32-chars!
JWT_ISSUER=distrocv-api
JWT_AUDIENCE=distrocv-client

# --- Google OAuth ---
GOOGLE_CLIENT_ID=your-dev-google-client-id

# --- Gemini AI ---
GEMINI_API_KEY=your-dev-gemini-api-key

# --- Frontend ---
VITE_API_URL=http://localhost:5000/api
